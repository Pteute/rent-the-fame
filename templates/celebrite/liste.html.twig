{% extends 'base.html.twig' %}

{% block title %}
{% endblock %}

{% block body %}

	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>


	<section class="celebrites">

		{% for liste, celebrite in celebrites %}
			<div class= "liste">
                <img src="{{ asset(celebrite.image) }}" alt="{{ celebrite.nom }}">
                <h3><strong>{{celebrite.prenom}} {{celebrite.nom}}</strong></h3>
			</div>
		{% endfor %}

	</section>

	{% endblock %}