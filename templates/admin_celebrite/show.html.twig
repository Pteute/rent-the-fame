{% extends 'admin.html.twig' %}

{% block title %}Celebrite{% endblock %}

{% block body %}
    <h1>Celebrite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ celebrite.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ celebrite.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ celebrite.prenom }}</td>
            </tr>
            <tr>
                <th>Metier</th>
                <td>{{ celebrite.metier }}</td>
            </tr>
            <tr>
                <th>Fame</th>
                <td>{{ celebrite.fame }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ celebrite.image }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ celebrite.createdAt ? celebrite.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ModifiedAt</th>
                <td>{{ celebrite.modifiedAt ? celebrite.modifiedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_celebrite_index') }}">back to list</a>

    <a href="{{ path('admin_celebrite_edit', {'id': celebrite.id}) }}">edit</a>

    {{ include('admin_celebrite/_delete_form.html.twig') }}
{% endblock %}
