{% form_theme form 'bootstrap_5_layout.html.twig' %}

{{ form_start(form) }}
{{ form_row(form.nom) }}
{{ form_row(form.prenom) }}
{{ form_row(form.metier) }}
{{ form_row(form.fame) }}
{{ form_row(form.imageUpload) }}
{% if celebrite.image != null %}
        <img src="{{ asset("uploads/celebrite/" ~ celebrite.image) }}" alt="{{ celebrite.nom }}" width="100px" height="100px"> 
{% endif %}
{{ form_row(form.activites) }}
<button class="btn btn-dark">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
